<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>

		<field name="id" type="text" default="0" 
		    label="COM_EVENTSHANDLER_FORM_LBL_EVENT_ID"
            readonly="true" 
            class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 

       <field name="created_by" type="createdby" default="" 
            label="COM_EVENTSHANDLER_FORM_LBL_EVENT_CREATED_BY"
            description="COM_EVENTSHANDLER_FORM_DESC_EVENT_CREATED_BY"  /> 

       <field name="name" type="text"
            label="COM_EVENTSHANDLER_FORM_LBL_EVENT_NAME"
            description="COM_EVENTSHANDLER_FORM_DESC_EVENT_NAME" 
            required="true" 
            filter="safehtml"
            maxlength="255" /> 
            
       <field name="alias" type="hidden"  
            filter="safehtml"/> 
	
		<field name="address" type="text"
			label="COM_EVENTSHANDLER_FORM_LBL_PLACE_ADDRESS"
			description="COM_EVENTSHANDLER_FORM_DESC_PLACE_ADDRESS" 
			required="false" 
            filter="safehtml"
            maxlength="255" /> 
           
		<field name="link_fb" type="text"
			label="COM_EVENTSHANDLER_FORM_LBL_PLACE_LINK_FB"
			description="COM_EVENTSHANDLER_FORM_DESC_PLACE_LINK_FB" 
			required="false" 
            filter="safehtml"
            maxlength="255" />
        
		<field name="link_tw" type="text"
			label="COM_EVENTSHANDLER_FORM_LBL_PLACE_LINK_TW"
			description="COM_EVENTSHANDLER_FORM_DESC_PLACE_LINK_TW" 
			required="false" 
            filter="safehtml"
            maxlength="255" /> 
		
		<field name="website" type="text"
			label="COM_EVENTSHANDLER_FORM_LBL_PLACE_WEBSITE"
			description="COM_EVENTSHANDLER_FORM_DESC_PLACE_WEBSITE" 
			required="false" 
            filter="safehtml"
            maxlength="255" /> 
       

         <field
	        name="state"
	        type="list"
	        label="JSTATUS"
	        description="JFIELD_PUBLISHED_DESC"
	        class="inputbox"
	        size="1"
	        default="1">
	        
        <option value="1">JPUBLISHED</option>
        <option value="0">JUNPUBLISHED</option>
    </field> 

                        <field name="checked_out" type="hidden" filter="unset" />
        <field name="checked_out_time" type="hidden" filter="unset" />

	</fieldset>

</form>
